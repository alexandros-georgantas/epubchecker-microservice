{
  "name": "@coko/epub-checker",
  "version": "1.0.0",
  "description": "Service that returns the validity of an EPUB",
  "main": "src/index.js",
  "scripts": {
    "cz": "git-cz",
    "migrate": "pubsweet migrate",
    "server": "pubsweet start:server",
    "release": "standard-version",
    "create:client": "node ./node_modules/@coko/service-auth/src/bin/createClient.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.coko.foundation/cokoapps/epubchecker"
  },
  "keywords": [
    "pubsweet",
    "pubsweet-server",
    "coko"
  ],
  "author": "Alexandros Georgantas",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@coko/server": "^1.3.0",
    "epubchecker": "^4.2.2",
    "winston": "2"
  },
  "devDependencies": {
    "@coko/lint": "^0.0.2",
    "standard-version": "^7.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
